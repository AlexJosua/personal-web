{{#> layout currentPage= "Blog"}}


<!-- BLOG DETAIL SECTION  -->
<article class="blog-detail">
  <h1>{{blog.title}}</h1>

  <p>{{formatDateToWIB blog.createdAt}} | {{user.name}} </p>

  <img src="/{{blog.image}}" alt="" />
  <p class="mt-3">
    {{blog.content}}
  </p>
</article>
